{
	"compilerOptions": {
		"resolveJsonModule": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "ES2017",
		"removeComments": true,
		"moduleResolution": "node",
		"outDir": "dist/js",
		"declaration": false,
		"module": "commonjs",
		"forceConsistentCasingInFileNames": false
	}
}